&uio_pruss2_mdio {
        reset-gpios = <&gpio5 8 GPIO_ACTIVE_LOW>,
                      <&gpio5 9 GPIO_ACTIVE_LOW>;
        reset-delay-us = <2>;   /* PHY datasheet states 1uS min */
};

&uio_pruss2_mdio {
         status = "okay";
         uio_pruss2_eth0_phy: uio_pruss2_eth0_phy@0 {
             reg = <0>;
         };

         uio_pruss2_eth1_phy: uio_pruss2_eth1_phy@1 {
              reg = <1>;
          };
};

