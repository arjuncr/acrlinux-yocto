		uio_pruss1_0_mem: uio_pruss1_0_mem {
			compatible = "ti,uio-module-drv";
			mem = <0x54474000 0x1000>,
			      <0x54462000 0x400>,
			      <0x54462400 0x100>;
			mem-names = "iram", "control", "debug";
			status = "okay";
		};
		uio_pruss1_1_mem: uio_pruss1_1_mem {
