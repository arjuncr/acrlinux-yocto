			      <0x54442000 0x1000>,
			      <0x54450000 0x8000>,
			      <0x54460000 0x2000>,
			      <0x54466000 0x2000>,
			      <0x5446e000 0x31c>,
			      <0x54472000 0x58>;
			mem-names = "dram0", "dram1", "shrdram2", "intc", "cfg",
